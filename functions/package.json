{
  "name": "functions",
  "description": "Cloud Functions for Firebase (Income Import Backend, exceljs-based)",
  "private": true,
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:ok": "npm run lint || node -e \"process.exit(0)\"",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.7.0",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.4.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0"
  }
}
